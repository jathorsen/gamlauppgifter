        <TextBox x:Name="txtPlatenumber" HorizontalAlignment="Left" Margin="231,231,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="67"/>
        <Button x:Name="btnPlatespotting" Content="Platespotting" HorizontalAlignment="Left" Margin="400,249,0,0" VerticalAlignment="Top" Width="75" Click="btnPlatespotting_Click"/>
        <Label x:Name="lblRandomNumber" HorizontalAlignment="Left" Margin="400,217,0,0" VerticalAlignment="Top" Width="75"/>
